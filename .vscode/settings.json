{
  "files.exclude": {

    ".vscode": false,

    "**/*.pyc": true,
    "**/__main__.py": true,
    "*.DS_Store": true,
    "*.Python": true,
    "**/__pycache__": true,
    "**/__init__.py": true,
    "**/celerybeat.pid": true,
    ".resp": true,
    ".envs": true,

    ".dockerignore": true,
    ".editorconfig": true,
    ".gitignore": true,
    ".pylintrc": true,
    "compose/": true,
    "local.yml": true,
    "merge_production_dotenvs_in_dotenv.py": true,
    "production.yml": true,
    "README.rst": true,

    "quasar/.quasar": true,
    "quasar/node_modules": true,
    "quasar/.eslintrc.js": true,
    "quasar/.postcssrc.js": true,
    "quasar/babel.config.js": true,
    "quasar/jsconfig.json": true,
    "quasar/package-lock.json": true,
    "quasar/pubic": true,
    "quasar/.eslintignore": true,

    "quda/.ipython": true,
    "quda/media": true,
    "quda/staticfiles": true,
    "manage.py": true,
    "quda/static/": true,
    "quda/**/migrations": true,
    "quda/contrib/": true,
    "quda/utils/": true,

    "config/api_router.py": true,
    "config/wsgi.py": true,
    "config/routing.py": true,
    "config/celery_app.py": true,

    "nginx/": true,
    "requirements/": true,
    "temp/": false,
    "database": true,
    "logs/": true,
    "docs/": true,
    ".ipython": true



  },
  "vetur.validation.template": false,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "vue"],
  "vetur.experimental.templateInterpolationService": true,
  "git.ignoreLimitWarning": true,
  "python.linting.pylintEnabled": false,
  "python.formatting.provider": "black",
  "python.pythonPath": "/usr/bin/python"
}
